{
    "name": "eliosfund/plaid-php-sdk",
    "keywords": ["plaid", "sdk", "php"],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Erickson",
            "email": "jon.erickson@eliosfund.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "saloonphp/saloon": "^3.0",
        "illuminate/support": "^11.10"
    },
    "require-dev": {
        "pestphp/pest": "^2.34",
        "phpstan/phpstan": "^1.11",
        "mockery/mockery": "^1.6",
        "pestphp/pest-plugin-type-coverage": "^2.8",
        "laravel/pint": "^1.16"
    },
    "autoload": {
        "psr-4": {
            "Plaid\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Plaid\\Test\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
